int i;

int main(void){
    int res;
    i = 0;
    if(!i && 3){
        res = facto(4);
    }
    else{
        res = rec(2);
    }
    putint(res);
    return res;
}

int rec(int n){
    if(n <= 1)
        return n;
    return rec(n - 1);
}

int facto(int n){
    if(n <= 1)
        return n;
    return n * facto(n-1);
}